Proceso calculadora 
	DEFINIR a , b COMO REAL;
	ESCRIBIR 'Ingrese el primer numero';
	LEER a;
	ESCRIBIR 'Ingrese el segundo numero';
	LEER b;
	DEFINIR s,r,m COMO REAL; 
	suma <- (a + b);
	resta <- (a - b);
	multiplicacion <- (a * b);
	ESCRIBIR 'Los resultados son: suma : ',s, ' resta : ',r, ' y multiplicacion : ',m;
FinProceso
